<?xml version="1.0" encoding="utf-8"?>
<guice>

    <module>com.maxifier.guice.bootstrap.xml.FooModule</module>

    <component>
        <interface>com.maxifier.guice.bootstrap.xml.TestInterface</interface>
        <class>com.maxifier.guice.bootstrap.xml.First</class>
    </component>

    <component>
        <interface>com.maxifier.guice.bootstrap.xml.TestInterface</interface>
        <annotation>com.maxifier.guice.bootstrap.xml.TestAnnotation</annotation>
        <class>com.maxifier.guice.bootstrap.xml.Second</class>
    </component>

    <component>
        <class>com.maxifier.guice.bootstrap.xml.Alone</class>
        <eager>true</eager>
    </component>

    <constant>
        <annotation>com.maxifier.guice.bootstrap.xml.Constant</annotation>
        <value>Hello world!</value>
    </constant>

    <component>
        <class>com.maxifier.guice.bootstrap.xml.AsEager</class>
        <eager>true</eager>
    </component>

    <component>
        <interface>com.maxifier.guice.bootstrap.xml.In</interface>
        <class>com.maxifier.guice.bootstrap.xml.InImpl</class>
        <scope>SINGLETON</scope>
    </component>

    <constant>
        <named>test.name</named>
        <value>Hello world!</value>
    </constant>

    <property>
        <name>test</name>
        <value>testValue</value>
    </property>

</guice>